text/ecmascript-6'>\n  import copy from 'copy-text-to-clipboard';\n  import {\n    Notification\n  } from 'element-ui';\n  import 'element-ui/lib/theme-chalk/index.css';\n  export default {\n    data() {\n      return {}\n    },\n    methods: {\n      // 将页面中的css代码块标签，加上点击复制功能\n      initCopyBoard() {\n        var pres = document.querySelectorAll(\".language-css.extra-class\");\n        var _this = this;\n        pres.forEach(function(pre, item) {\n          pre.addEventListener(\"click\", function(e) {\n            var text = pre.innerText;\n            var msg =  text.slice(0, 30) + '...';\n            copy(text) ? Notification.success({\n              title: '复制成功',\n              message: msg,\n            }) : Notification.error({\n              title: '复制失败',\n              message: msg,\n            })\n          })\n        })\n      }\n    },\n    mounted() {\n      this.initCopyBoard();\n      console.log('help');\n    }\n  }\n</script>\n\n\n\n"],"sourceRoot":"synax-helpbook/.vuepress/components"}]}